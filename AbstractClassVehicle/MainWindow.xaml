<Window x:Class="AbstractClassVehicle.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AbstractClassVehicle"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <TextBlock Text="CAR" Name="T1" Canvas.Top="25" Canvas.Left="300" FontSize="25" FontStyle="Italic" FontWeight="Bold"></TextBlock>
        <Button Name="B1" Canvas.Top="100" Canvas.Left="100" Height="30" Width="80" Click="B1_Click">Speed</Button>
        <Button Name="B2" Canvas.Top="100" Canvas.Left="300" Height="30" Width="80" Click="B2_Click">Wheels</Button>
        <Button Name="B3" Canvas.Top="100" Canvas.Left="500" Height="30" Width="80" Click="B3_Click">Name</Button>
        <TextBlock Text="BIKE" Name="T2" Canvas.Top="200" Canvas.Left="300" FontSize="25" FontStyle="Italic" FontWeight="Bold"></TextBlock>
        <Button Name="B4" Canvas.Top="300" Canvas.Left="100" Height="30" Width="80" Click="B4_Click">Speed</Button>
        <Button Name="B5" Canvas.Top="300" Canvas.Left="300" Height="30" Width="80" Click="B5_Click">Wheels</Button>
        <Button Name="B6" Canvas.Top="300" Canvas.Left="500" Height="30" Width="80" Click="B6_Click">Name</Button>
    </Canvas>
</Window>
